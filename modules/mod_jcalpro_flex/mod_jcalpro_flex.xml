<?xml version="1.0" encoding="utf-8"?>
<extension type="module" version="2.5" client="site" method="upgrade">
  <name>mod_jcalpro_flex</name>
  <author>Anything Digital</author>
  <authorEmail>admin@anything-digital.com</authorEmail>
  <authorUrl>http://anything-digital.com</authorUrl>
  <creationDate>2015-04-21</creationDate>
  <copyright>Copyright (c) 2006-2012 Anything-Digital.com</copyright>
  <license>http://www.gnu.org/licenses/gpl-2.0.html GNU/GPL</license>
  <version>2.0.0.4081</version>
  <description>MOD_JCALPRO_FLEX_XML_DESCRIPTION</description>
  <files>
    <filename module="mod_jcalpro_flex">mod_jcalpro_flex.php</filename>
    <folder>fields</folder>
    <folder>forms</folder>
    <folder>tmpl</folder>
    <filename>helper.php</filename>
    <filename>index.html</filename>
    <filename>mod_jcalpro_flex.xml</filename>
  </files>
  <media destination="jcalpro/modules/flex" folder="media">
    <folder>css</folder>
    <folder>js</folder>
    <filename>index.html</filename>
  </media>
  <languages folder="language">
    <language tag="en-GB">en-GB/en-GB.mod_jcalpro_flex.ini</language>
    <language tag="en-GB">en-GB/en-GB.mod_jcalpro_flex.sys.ini</language>
    <language tag="pt-BR">pt-BR/pt-BR.mod_jcalpro_flex.sys.ini</language>
    <language tag="es-ES">es-ES/es-ES.mod_jcalpro_flex.sys.ini</language>
    <language tag="nl-NL">nl-NL/nl-NL.mod_jcalpro_flex.sys.ini</language>
    <language tag="sv-SE">sv-SE/sv-SE.mod_jcalpro_flex.sys.ini</language>
    <language tag="no-NO">no-NO/no-NO.mod_jcalpro_flex.sys.ini</language>
    <language tag="ro-RO">ro-RO/ro-RO.mod_jcalpro_flex.sys.ini</language>
    <language tag="de-DE">de-DE/de-DE.mod_jcalpro_flex.sys.ini</language>
    <language tag="ru-RU">ru-RU/ru-RU.mod_jcalpro_flex.sys.ini</language>
    <language tag="da-DK">da-DK/da-DK.mod_jcalpro_flex.sys.ini</language>
    <language tag="fr-FR">fr-FR/fr-FR.mod_jcalpro_flex.sys.ini</language>
    <language tag="pl-PL">pl-PL/pl-PL.mod_jcalpro_flex.sys.ini</language>
    <language tag="pt-BR">pt-BR/pt-BR.mod_jcalpro_flex.ini</language>
    <language tag="es-ES">es-ES/es-ES.mod_jcalpro_flex.ini</language>
    <language tag="nl-NL">nl-NL/nl-NL.mod_jcalpro_flex.ini</language>
    <language tag="sv-SE">sv-SE/sv-SE.mod_jcalpro_flex.ini</language>
    <language tag="no-NO">no-NO/no-NO.mod_jcalpro_flex.ini</language>
    <language tag="ro-RO">ro-RO/ro-RO.mod_jcalpro_flex.ini</language>
    <language tag="de-DE">de-DE/de-DE.mod_jcalpro_flex.ini</language>
    <language tag="ru-RU">ru-RU/ru-RU.mod_jcalpro_flex.ini</language>
    <language tag="da-DK">da-DK/da-DK.mod_jcalpro_flex.ini</language>
    <language tag="fr-FR">fr-FR/fr-FR.mod_jcalpro_flex.ini</language>
    <language tag="pl-PL">pl-PL/pl-PL.mod_jcalpro_flex.ini</language>
  </languages>
  <compatibility>
    <version>2</version>
    <version>3</version>
  </compatibility>
  <config>
    <fields name="params">
      <fieldset name="basic">
        <field name="layout" type="list" default="slider" label="MOD_JCALPRO_FLEX_LAYOUT" description="MOD_JCALPRO_FLEX_LAYOUT_DESC">
          <option value="sliders">MOD_JCALPRO_FLEX_LAYOUT_OPTION_SLIDERS</option>
          <option value="tabs">MOD_JCALPRO_FLEX_LAYOUT_OPTION_TABS</option>
          <option value="vertical">MOD_JCALPRO_FLEX_LAYOUT_OPTION_VERTICAL</option>
          <option value="horizontal">MOD_JCALPRO_FLEX_LAYOUT_OPTION_HORIZONTAL</option>
        </field>
        <field name="debug" id="debug" type="radio" label="MOD_JCALPRO_FLEX_DEBUG" description="MOD_JCALPRO_FLEX_DEBUG_DESC" default="0" class="btn-group btn-group-yesno">
          <option value="1">JYES</option>
          <option value="0">JNO</option>
        </field>
      </fieldset>
      <fieldset name="panels" addfieldpath="/modules/mod_jcalpro_flex/fields">
        <field name="panels" type="jcalflexpanel" label="MOD_JCALPRO_FLEX_PANELS" description="MOD_JCALPRO_FLEX_PANELS_DESC"/>
      </fieldset>
      <fieldset name="advanced">
        <field name="moduleclass_sfx" type="text" label="COM_MODULES_FIELD_MODULECLASS_SFX_LABEL" description="COM_MODULES_FIELD_MODULECLASS_SFX_DESC"/>
        <field name="cache" type="list" default="1" label="COM_MODULES_FIELD_CACHING_LABEL" description="COM_MODULES_FIELD_CACHING_DESC">
          <option value="1">JGLOBAL_USE_GLOBAL</option>
          <option value="0">COM_MODULES_FIELD_VALUE_NOCACHING</option>
        </field>
        <field name="cache_time" type="text" default="900" label="COM_MODULES_FIELD_CACHE_TIME_LABEL" description="COM_MODULES_FIELD_CACHE_TIME_DESC"/>
        <field name="cachemode" type="hidden" default="static">
          <option value="static"/>
        </field>
      </fieldset>
    </fields>
  </config>
</extension>
