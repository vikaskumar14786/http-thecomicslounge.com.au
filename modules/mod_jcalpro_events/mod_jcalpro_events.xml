        <field name="itemid" type="text" label="MOD_JCALPRO_EVENTS_ITEMID" description="MOD_JCALPRO_EVENTS_ITEMID_DESC"/>
        <field name="empty_html" id="empty_html" type="textarea" label="MOD_JCALPRO_EVENTS_EMPTY_HTML" description="MOD_JCALPRO_EVENTS_EMPTY_HTML_DESC" default="" filter="JComponentHelper::filterText" asset_id="com_jcalpro"/>
        <field name="debug" id="debug" type="radio" label="MOD_JCALPRO_EVENTS_DEBUG" description="MOD_JCALPRO_EVENTS_DEBUG_DESC" default="0" class="btn-group btn-group-yesno">
          <option value="1">JYES</option>
          <option value="0">JNO</option>
        </field>
