        <field name="filter_category_invert" id="filter_category_invert" type="radio" label="MOD_JCALPRO_EVENTS_FILTER_CATEGORY_INVERT" description="MOD_JCALPRO_EVENTS_FILTER_CATEGORY_INVERT_DESC" default="0" class="btn-group btn-group-yesno btn-group-yesno-alt">
          <option value="1">MOD_JCALPRO_EVENTS_FILTER_CATEGORY_INVERT_OPTION_EXCLUDE</option>
