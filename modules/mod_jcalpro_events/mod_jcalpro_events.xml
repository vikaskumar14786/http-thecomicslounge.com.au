      <fieldset name="basic">
        <field name="featured" type="list" label="MOD_JCALPRO_EVENTS_FEATURED" description="MOD_JCALPRO_EVENTS_FEATURED_DESC" default="1">
          <option value="0">MOD_JCALPRO_EVENTS_FEATURED_OPTION_IGNORE</option>
          <option value="1">MOD_JCALPRO_EVENTS_FEATURED_OPTION_HIGHLIGHT</option>
          <option value="2">MOD_JCALPRO_EVENTS_FEATURED_OPTION_ONLY</option>
        </field>
        <field name="filter_date_range" type="list" label="MOD_JCALPRO_EVENTS_FILTER_DATE_RANGE" description="MOD_JCALPRO_EVENTS_FILTER_DATE_RANGE_DESC" default="2">
          <option value="0">MOD_JCALPRO_EVENTS_FILTER_DATE_RANGE_OPTION_NO_FILTER</option>
          <option value="14">MOD_JCALPRO_EVENTS_FILTER_DATE_RANGE_OPTION_ONGOING</option>
          <option value="1">MOD_JCALPRO_EVENTS_FILTER_DATE_RANGE_OPTION_PAST_EVENTS</option>
