          <option value="5">MOD_JCALPRO_EVENTS_FILTER_DATE_RANGE_OPTION_NEXT_WEEK</option>
          <option value="15">MOD_JCALPRO_EVENTS_FILTER_DATE_RANGE_OPTION_NEXT_2_WEEKS</option>
          <option value="16">MOD_JCALPRO_EVENTS_FILTER_DATE_RANGE_OPTION_NEXT_3_WEEKS</option>
          <option value="6">MOD_JCALPRO_EVENTS_FILTER_DATE_RANGE_OPTION_THIS_MONTH</option>
          <option value="7">MOD_JCALPRO_EVENTS_FILTER_DATE_RANGE_OPTION_LAST_MONTH</option>
          <option value="8">MOD_JCALPRO_EVENTS_FILTER_DATE_RANGE_OPTION_NEXT_MONTH</option>
          <option value="17">MOD_JCALPRO_EVENTS_FILTER_DATE_RANGE_OPTION_NEXT_2_MONTHS</option>
          <option value="18">MOD_JCALPRO_EVENTS_FILTER_DATE_RANGE_OPTION_NEXT_3_MONTHS</option>
          <option value="19">MOD_JCALPRO_EVENTS_FILTER_DATE_RANGE_OPTION_THIS_YEAR</option>
          <option value="20">MOD_JCALPRO_EVENTS_FILTER_DATE_RANGE_OPTION_LAST_YEAR</option>
          <option value="21">MOD_JCALPRO_EVENTS_FILTER_DATE_RANGE_OPTION_NEXT_YEAR</option>
        </field>
