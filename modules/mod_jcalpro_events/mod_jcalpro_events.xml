          <option value="1">JYES</option>
          <option value="0">JNO</option>
        </field>
        <field name="display_readmore" id="display_readmore" type="radio" label="MOD_JCALPRO_EVENTS_DISPLAY_READMORE" description="MOD_JCALPRO_EVENTS_DISPLAY_READMORE_DESC" default="1" class="btn-group btn-group-yesno">
          <option value="1">JYES</option>
          <option value="0">JNO</option>
        </field>
