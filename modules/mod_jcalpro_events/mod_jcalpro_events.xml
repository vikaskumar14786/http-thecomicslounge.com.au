          <option value="1">JYES</option>
          <option value="0">JNO</option>
        </field>
        <field name="display_description" id="display_description" type="radio" label="MOD_JCALPRO_EVENTS_DISPLAY_DESCRIPTION" description="MOD_JCALPRO_EVENTS_DISPLAY_DESCRIPTION_DESC" default="1" class="btn-group btn-group-yesno">
          <option value="1">JYES</option>
          <option value="0">JNO</option>
        </field>
        <field name="display_title" id="display_title" type="radio" label="MOD_JCALPRO_EVENTS_DISPLAY_TITLE" description="MOD_JCALPRO_EVENTS_DISPLAY_TITLE_DESC" default="1" class="btn-group btn-group-yesno">
          <option value="1">JYES</option>
          <option value="0">JNO</option>
        </field>
        <field name="show_months" id="show_months" type="radio" label="MOD_JCALPRO_EVENTS_SHOW_MONTHS" description="MOD_JCALPRO_EVENTS_SHOW_MONTHS_DESC" default="0" class="btn-group btn-group-yesno">
          <option value="1">JYES</option>
          <option value="0">JNO</option>
        </field>
        <field name="filter_location" id="filter_location" type="jcallocationlist" label="MOD_JCALPRO_EVENTS_FILTER_LOCATION" description="MOD_JCALPRO_EVENTS_FILTER_LOCATION_DESC" multiple="multiple" size="6">
          <option value="0">MOD_JCALPRO_EVENTS_FILTER_LOCATION_WITHOUT</option>
          <option value="-1">MOD_JCALPRO_EVENTS_FILTER_LOCATION_ALL</option>
          <option value="-2">MOD_JCALPRO_EVENTS_FILTER_LOCATION_WITH</option>
        </field>
        <field name="display_location" id="display_location" type="radio" label="MOD_JCALPRO_EVENTS_DISPLAY_LOCATION" description="MOD_JCALPRO_EVENTS_DISPLAY_LOCATION_DESC" default="0" class="btn-group btn-group-yesno">
          <option value="1">JYES</option>
          <option value="0">JNO</option>
